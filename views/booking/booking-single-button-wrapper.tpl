<section class="car-park-detail__booking-wrapper" data-searchShowId="booking-single-button-wrapper">
    <div class="wrapper-map-left">
        <div id="cannot_book_content" class="car-park-detail__cannot-book" style="display:none">
            Cannot add current car park item to the cart because it is from a different country. Please complete the booking in the cart before adding this item to the cart.
        </div>
        <div class="car-park-detail__book">
            <div class="car-park-detail__cta" data-btnAction="search-continue">
                Continue
            </div>
            <div class="car-park-detail__cta" data-btnAction="add-to-cart">
                Add to cart
            </div>
            <div class="car-park-detail__cta car-park-detail__cta--cart-redirect" data-btnAction="go-to-cart">
                Go to cart
            </div>
            <div class="car-park-detail__cta car-park-detail__cta--cart-redirect" data-btnAction="make-another-booking">
                Make another booking
            </div>
        </div>
    </div>
</section>